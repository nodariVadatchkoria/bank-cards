  "keywords": [
    "bank-cards",
    "catalog",
    "nextjs",
    "typescript",
    "react",
    "crud",
    "banking"
  ],
  "author": "Your Name",
  "license": "MIT",
  "homepage": "https://github.com/yourusername/bank-cards#readme",
